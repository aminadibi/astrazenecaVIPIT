	keywords = {Female, Humans, Male, Middle Aged, Aged, Aged, 80 and over, Pulmonary Disease, Chronic Obstructive, Cluster Analysis, Cohort Studies, Cross-Sectional Studies, Genome-Wide Association Study, Spirometry},
	pages = {89},
