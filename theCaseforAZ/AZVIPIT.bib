	keywords = {Adult, Asthma, Female, Humans, Male, Middle Aged, Aged, Netherlands, Comorbidity, Lung Diseases, Obstructive, Logistic Models, Hypertension, Gait Disorders, Neurologic, Heart Diseases, Sleep Initiation and Maintenance Disorders},
