	keywords = {Asthma, Humans, Reproducibility of Results, Follow-Up Studies, Models, Biological, Disease Progression, Severity of Illness Index, Markov Chains, Patient Acceptance of Health Care, Artificial Intelligence, Databases as Topic},
	pages = {249--255},
