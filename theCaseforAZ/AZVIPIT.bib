	keywords = {Adolescent, Adult, Female, Humans, Male, Middle Aged, Smoking, Young Adult, Aged, Aged, 80 and over, Prospective Studies, Lung, Forced Expiratory Volume, Health Surveys, Smoking Cessation, Aging},
	pages = {3--10},
