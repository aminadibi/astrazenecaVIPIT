	pmcid = {PMC27319},
	keywords = {Adult, Asthma, Humans, Middle Aged, Confidence Intervals, Time Factors, Peak Expiratory Flow Rate, Diet, Reducing, Obesity, Weight Loss, Analysis of Variance, Statistics, Nonparametric},
	pages = {827--832},
