	journal = {International Journal of Chronic Obstructive Pulmonary Disease},
	author = {Pothirat, Chaicharn and Liwsrisakun, Chalerm and Bumroongkit, Chaiwat and Deesomchok, Athavudh and Theerakittikul, Theerakorn and Limsukon, Atikun},
