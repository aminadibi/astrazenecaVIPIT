	pages = {851--859},
	file = {PubMed Central Full Text PDF:C\:\\Users\\amin\\Zotero\\storage\\UMG7MGKL\\Liu et al. - 2015 - Body mass index, respiratory conditions, asthma, a.pdf:application/pdf;PubMed Central Full Text PDF:C\:\\Users\\amin\\Zotero\\storage\\6SLFT4RV\\Liu et al. - 2015 - Body mass index, respiratory conditions, asthma, a.pdf:application/pdf},
