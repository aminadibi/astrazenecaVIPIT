	author = {Anthonisen, N. R. and Connett, J. E. and Kiley, J. P. and Altose, M. D. and Bailey, W. C. and Buist, A. S. and Conway, W. A. and Enright, P. L. and Kanner, R. E. and O'Hara, P.},
