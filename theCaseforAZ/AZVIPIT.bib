	pmid = {9877485},
	keywords = {Adolescent, Adult, Asthma, Female, Humans, Male, Middle Aged, Aged, Aged, 80 and over, Severity of Illness Index, Spain, Health Care Costs, Cost of Illness},
	pages = {1322--1326},
}

