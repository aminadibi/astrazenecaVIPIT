	urldate = {2019-08-27},
	journal = {Scandinavian Journal of Primary Health Care},
	author = {Stratelis, Georgios and Fransson, Sven-Göran and Schmekel, Birgitta and Jakobsson, Per and Mölstad, Sigvard},
	year = {2008},
