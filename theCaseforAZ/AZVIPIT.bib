	pmid = {25128045},
	keywords = {Female, Humans, Male, Smoking, Pulmonary Disease, Chronic Obstructive, Disease Progression, Severity of Illness Index, cost-effectiveness, Cost-Benefit Analysis, COPD, Quality-Adjusted Life Years, Models, Economic, validation, model, Uncertainty},
	pages = {525--536},
}

