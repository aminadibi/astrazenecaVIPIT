	journal = {Frontiers in Neurology},
	author = {White, Alice J. and Wijeyekoon, Ruwani S. and Scott, Kirsten M. and Gunawardana, Nushan P. and Hayat, Shaista and Solim, I. H. and McMahon, H. T. and Barker, Roger A. and Williams-Gray, Caroline H.},
