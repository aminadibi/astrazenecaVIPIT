@article{reijula_long-term_2020,
	title = {Long-term trends of asthma, allergic rhinitis and atopic eczema in young {Finnish} men: a retrospective analysis, 1926–2017},
	volume = {56},
	copyright = {Copyright ©ERS 2020. https://www.ersjournals.com/user-licence},
