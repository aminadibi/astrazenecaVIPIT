	keywords = {Adult, Asthma, Female, Humans, Male, Middle Aged, Risk Factors, Age Distribution, Canada, Proportional Hazards Models, Patient Readmission, Income, Socioeconomic Factors, Residence Characteristics, Sex Distribution, Educational Status, Length of Stay},
	pages = {446--453},
