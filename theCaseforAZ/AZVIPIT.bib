@article{feldman_new_2009-1,
	title = {New {Approaches} to {Studying} {Problem} {Behaviors}: {A} {Comparison} of {Methods} for {Modeling} {Longitudinal}, {Categorical} {Adolescent} {Drinking} {Data}},
