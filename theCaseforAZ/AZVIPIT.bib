	keywords = {Adolescent, Adult, Asthma, atopy, Child, Female, Humans, Longitudinal Studies, lung function, Male, Middle Aged, remission, Respiratory Function Tests, Skin Tests, Smoking, Surveys and Questionnaires, Young Adult, Outcome Assessment (Health Care)},
	pages = {1572--1578.e3},
}

