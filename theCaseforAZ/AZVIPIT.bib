	keywords = {Female, Humans, Male, Middle Aged, Models, Theoretical, Aged, Cross-Sectional Studies, United States, Quality of Life, Health Status, Questionnaires, Linear Models},
	pages = {5--15},
}

@article{penninx_psychological_1996,
	title = {Psychological status among elderly people with chronic diseases: {Does} type of disease play a part?},
	volume = {40},
	issn = {0022-3999},
