	journal = {European Respiratory Journal},
	author = {Vestbo, J. and Anderson, W. and Coxson, H. O. and Crim, C. and Dawber, F. and Edwards, L. and Hagan, G. and Knobil, K. and Lomas, D. A. and MacNee, W. and Silverman, E. K. and Tal-Singer, R. and Investigators, on behalf of the ECLIPSE},
