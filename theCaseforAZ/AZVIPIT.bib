	keywords = {clinical trials, precision medicine, prediction modeling, regression analysis, subgroup analysis},
	pages = {1362--1373},
	file = {Submitted Version:C\:\\Users\\amin\\Zotero\\storage\\SQ2L459M\\Sadatsafavi et al. - 2020 - A threshold-free summary index for quantifying the.pdf:application/pdf},
}

