	pmid = {15679714},
	keywords = {Asthma, Humans, Anti-Asthmatic Agents, Severity of Illness Index, Antibodies, Anti-Idiotypic, Antibodies, Monoclonal, Antibodies, Monoclonal, Humanized, Randomized Controlled Trials as Topic, Emergency Medical Services, Immunoglobulin E},
