	keywords = {Adult, Asthma, Humans, Middle Aged, Smoking, Aged, Biomarkers, Netherlands, Pulmonary Disease, Chronic Obstructive, Spirometry, Risk, Survival Analysis, Inflammation Mediators, airway obstruction, death rate, lung function tests, Respiratory hypersensitivity, Respiratory Hypersensitivity, Syndrome},
	pages = {1571--1579.e10},
}

