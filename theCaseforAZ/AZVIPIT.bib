@article{mannino_pre-_2011,
	title = {Pre- and post-bronchodilator lung function as predictors of mortality in the {Lung} {Health} {Study}},
	volume = {12},
	issn = {1465-993X},
	url = {https://doi.org/10.1186/1465-9921-12-136},
	doi = {10.1186/1465-9921-12-136},
