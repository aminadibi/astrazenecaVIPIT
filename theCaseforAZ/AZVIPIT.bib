	keywords = {Female, Humans, Male, Prognosis, Aged, Prospective Studies, Treatment Outcome, Multivariate Analysis, Predictive Value of Tests, Severity of Illness Index, Comorbidity, Myocardial Infarction, Survival Analysis, Proportional Hazards Models, Time Factors},
	pages = {842--849},
