	journal = {Thorax},
	author = {Sin, D. D. and Wu, L. and Anderson, J. A. and Anthonisen, N. R. and Buist, A. S. and Burge, P. S. and Calverley, P. M. and Connett, J. E. and Lindmark, B. and Pauwels, R. A. and Postma, D. S. and Soriano, J. B. and Szafranski, W. and Vestbo, J.},
