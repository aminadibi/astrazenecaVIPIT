	pmid = {17167067},
	keywords = {Humans, Models, Biological, alpha-Synuclein, Animals, Anthracenes, Blotting, Western, Butadienes, Chromones, Enzyme Inhibitors, Fibroblast Growth Factor 2, Gene Expression Regulation, Imidazoles, Introns, JNK Mitogen-Activated Protein Kinases, Morpholines, Nerve Growth Factor, Nitriles, p38 Mitogen-Activated Protein Kinases, PC12 Cells, Phosphatidylinositol 3-Kinases, Pyridines, Rats, Reverse Transcriptase Polymerase Chain Reaction, Transcription, Genetic},
