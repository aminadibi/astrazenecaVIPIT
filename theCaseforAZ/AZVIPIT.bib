	file = {Bertens et al. - 2013 - Development and validation of a model to predict t.pdf:C\:\\Users\\amin\\Zotero\\storage\\GXUALK3Z\\Bertens et al. - 2013 - Development and validation of a model to predict t.pdf:application/pdf},
