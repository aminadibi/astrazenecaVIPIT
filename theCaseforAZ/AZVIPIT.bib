	journal = {Revista Española De Cardiología},
	author = {Núñez, Julio E. and Núñez, Eduardo and Fácila, Lorenzo and Bertomeu, Vicente and Llàcer, Angel and Bodí, Vicent and Sanchis, Juan and Sanjuán, Rafael and Blasco, María L. and Consuegra, Luciano and Martínez, Angel and Chorro, Francisco J.},
