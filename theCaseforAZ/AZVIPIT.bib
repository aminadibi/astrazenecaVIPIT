	pmid = {28365676},
	keywords = {Adolescent, Adult, Female, Humans, Male, Young Adult, Cross-Sectional Studies, Bone Density, Body Mass Index, Forced Expiratory Volume, bone mineral density, body mass, Healthy Volunteers, physical activity, Students, Ultrasonography, young adults},
