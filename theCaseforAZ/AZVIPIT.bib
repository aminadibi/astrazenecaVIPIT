	pmid = {16443705},
	keywords = {Asthma, Prognosis, BMI, body mass index, FEV1, forced expiratory volume in 1 second, FVC, forced vital capacity, ICS, inhaled corticosteroids, inhaled corticosteroids, ventilatory function},
	pages = {100--104},
	file = {Full Text PDF:C\:\\Users\\amin\\Zotero\\storage\\TKS4AE3U\\Lange et al. - 2006 - Inhaled corticosteroids and decline of lung functi.pdf:application/pdf;Snapshot:C\:\\Users\\amin\\Zotero\\storage\\26KDGWDB\\100.html:text/html},
