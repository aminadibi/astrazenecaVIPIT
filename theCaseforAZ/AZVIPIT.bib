	pmid = {28472184},
	pmcid = {PMC5417635},
	file = {PubMed Central Full Text PDF:C\:\\Users\\amin\\Zotero\\storage\\H4ZF3A4F\\Pérez-Padilla et al. - 2017 - Lung function decline in subjects with and without.pdf:application/pdf},
}

