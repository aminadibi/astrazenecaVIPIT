	pmid = {19321196},
	keywords = {Adult, Asthma, Female, Humans, Male, Smoking, Follow-Up Studies, Lung, Body Mass Index, Forced Expiratory Volume, Europe, Weight Gain},
	pages = {1069--1074, 1074.e1--4},
