	pmid = {20064088},
	pmcid = {PMC3877919},
	keywords = {Adult, Humans, Longitudinal Studies, Male, Middle Aged, Respiratory Function Tests, Aged, Prospective Studies, Pulmonary Disease, Chronic Obstructive, Animals, Linear Models, C-Reactive Protein, Germany, Air Pollution, Air Pollutants, Carbon Monoxide, Environmental Monitoring, Leukocyte Count, Lymphocyte Count, Nitrogen Dioxide, Oxidants, Photochemical, Particle Size, Rabbits},
