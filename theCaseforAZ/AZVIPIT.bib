	urldate = {2019-02-07},
	journal = {Chest},
	author = {Ferreira, Ivone M. and Brooks, Dina and Lacasse, Yves and Goldstein, Roger S.},
	month = mar,
	year = {2000},
	keywords = {COPD, BMI = body mass index, CI = confidence interval, meta-analysis, nutrition, RCT = randomized controlled trials, respiratory rehabilitation, systematic review},
