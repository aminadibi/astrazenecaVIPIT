	keywords = {Adolescent, Adult, Asthma, Child, Female, Humans, Male, Young Adult, Mortality, Global Health, World Health Organization, Child, Preschool, Data Interpretation, Statistical, Cause of Death, Databases, Factual},
	pages = {935--945},
