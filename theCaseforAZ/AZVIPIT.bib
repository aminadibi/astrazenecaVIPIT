@article{xiang_asthma_2020,
	title = {Asthma {Exacerbation} {Prediction} and {Risk} {Factor} {Analysis} {Based} on a {Time}-{Sensitive}, {Attentive} {Neural} {Network}: {Retrospective} {Cohort} {Study}},
