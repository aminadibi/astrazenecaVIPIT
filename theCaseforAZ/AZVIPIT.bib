	pmid = {26783324},
	keywords = {Adolescent, Child, Female, Humans, Male, Multivariate Analysis, Regression Analysis, Child, Preschool, Hospitalization, Retrospective Studies, Hospital Costs, Hospitals, Pediatric},
	pages = {e20151829},
